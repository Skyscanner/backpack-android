{
  "private": true,
  "name": "backpack-android",
  "version": "0.0.3",
  "engines": {
    "node": "^8.9.4",
    "npm": "^5.6.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "preinstall": "node -e \"$(curl -fsSL https://raw.githubusercontent.com/Skyscanner/ensure-node-env/master/dist/index.js)\"",
    "prerelease:env": ": ${BINTRAY_USER?Need to set BINTRAY_USER} ${BINTRAY_KEY?Need to set BINTRAY_KEY}",
    "prerelease": "npm run prerelease:env",
    "release": "node release.js",
    "test": "eslint . --ext .js,.jsx",
    "prebuild": "npm test",
    "build": "gulp",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skyscanner/backpack-android.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Skyscanner/backpack-android/issues"
  },
  "homepage": "https://github.com/Skyscanner/backpack-android#readme",
  "devDependencies": {
    "bpk-tokens": "^26.7.2",
    "del": "^3.0.0",
    "eslint-config-skyscanner-with-prettier": "^0.3.0",
    "eslint_d": "^5.3.1",
    "gulp": "^3.9.1",
    "gulp-data": "^1.3.1",
    "gulp-nunjucks": "^3.1.1",
    "gulp-rename": "^1.2.2",
    "lodash": "^4.17.4",
    "merge2": "^1.2.1",
    "through2": "^2.0.3",
    "tinycolor2": "^1.4.1"
  },
  "dependencies": {
    "gulp-util": "^3.0.8",
    "inquirer": "^5.0.0",
    "run-sequence": "^2.2.1",
    "semver": "^5.5.0",
    "release-it": "^7.0.0",
    "replace-in-file": "^3.1.0",
    "node-fetch": "^2.0.0"
  }
}
