    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_marginTop="@dimen/bpkSpacingMd"
    android:layout_marginStart="@dimen/bpkSpacingBase"
    app:layout_constraintTop_toBottomOf="@id/primaryBasicSubheader"
    app:layout_constraintStart_toStartOf="parent">

    <net.skyscanner.backpack.button.BpkButton
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Default"
      android:layout_marginStart="@dimen/bpkSpacingMd"
      app:buttonSize="basic"
      app:buttonType="primary"/>

    <net.skyscanner.backpack.button.BpkButton
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Disabled"
      android:enabled="false"
      android:layout_marginStart="@dimen/bpkSpacingMd"
      app:buttonSize="basic"
      app:buttonType="primary"/>

    <net.skyscanner.backpack.button.BpkButton
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="With icon"
      android:layout_marginStart="@dimen/bpkSpacingMd"
      app:buttonIcon="@drawable/bpk_weather"
      app:buttonSize="basic"
      app:buttonType="primary"/>

    <net.skyscanner.backpack.button.BpkButton
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="@dimen/bpkSpacingMd"
      app:buttonIcon="@drawable/bpk_weather"
      app:buttonSize="basic"
