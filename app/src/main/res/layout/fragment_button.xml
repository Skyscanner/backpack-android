  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:ignore="HardcodedText">

  <net.skyscanner.backpack.text.BpkText
    android:id="@+id/primaryHeader"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Primary"
    android:layout_marginTop="@dimen/bpkSpacingBase"
    android:layout_marginStart="@dimen/bpkSpacingBase"
    android:textStyle="bold"
    android:textSize="16sp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"/>

  <net.skyscanner.backpack.text.BpkText
    android:id="@+id/primaryBasicSubheader"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Basic"
    android:layout_marginTop="@dimen/bpkSpacingSm"
    android:layout_marginStart="@dimen/bpkSpacingBase"
    android:layout_marginBottom="@dimen/bpkSpacingSm"
    android:textSize="14sp"
    app:layout_constraintTop_toBottomOf="@id/primaryHeader"
    app:layout_constraintStart_toStartOf="parent"/>

