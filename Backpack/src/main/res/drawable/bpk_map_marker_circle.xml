<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_selected="true">
    <shape android:shape="oval">
      <size
        android:width="@dimen/bpkSpacingBase"
        android:height="@dimen/bpkSpacingBase" />
    </shape>
  </item>
  <item>
    <shape android:shape="oval">
      <size
        android:width="@dimen/bpkSpacingBase"
        android:height="@dimen/bpkSpacingBase" />
      <solid android:color="@color/bpkSkyBlue" />
      <stroke android:width="2dp" android:color="@color/bpkWhite" />
    </shape>
  </item>
</selector>

